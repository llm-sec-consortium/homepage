[{"key": "db4cf9f6a653d5c15973e836c800ea47743251ae", "ss_url": "https://www.semanticscholar.org/paper/db4cf9f6a653d5c15973e836c800ea47743251ae", "title": "Prompt Injection attack against LLM-integrated Applications", "venue": "arXiv.org", "year": 2023, "citation_count": 44, "authors": [{"author_id": "2153627626", "author_name": "Yi Liu", "author_highlight": true, "author_url": "https://www.semanticscholar.org/author/2153627626"}, {"author_id": "73776889", "author_name": "Gelei Deng", "author_highlight": true, "author_url": "https://www.semanticscholar.org/author/73776889"}, {"author_id": "22799258", "author_name": "Yuekang Li", "author_highlight": true, "author_url": "https://www.semanticscholar.org/author/22799258"}, {"author_id": "3088630", "author_name": "Kailong Wang", "author_highlight": true, "author_url": "https://www.semanticscholar.org/author/3088630"}, {"author_id": "2146331573", "author_name": "Tianwei Zhang", "author_highlight": false, "author_url": "https://www.semanticscholar.org/author/2146331573"}, {"author_id": "39584070", "author_name": "Yepang Liu", "author_highlight": false, "author_url": "https://www.semanticscholar.org/author/39584070"}, {"author_id": "51225422", "author_name": "Haoyu Wang", "author_highlight": false, "author_url": "https://www.semanticscholar.org/author/51225422"}, {"author_id": "2124949853", "author_name": "Yanhong Zheng", "author_highlight": false, "author_url": "https://www.semanticscholar.org/author/2124949853"}, {"author_id": "2152798056", "author_name": "Yang Liu", "author_highlight": false, "author_url": "https://www.semanticscholar.org/author/2152798056"}], "written_by_us": true, "tldr": "This study deconstructs the complexities and implications of prompt injection attacks on actual LLM-integrated applications and forms HouYi, a novel black-box prompt injection attack technique, which draws inspiration from traditional web injection attacks."}, {"key": "6987c95f7054d2653178ac93df52aa3c0b99fcf5", "ss_url": "https://www.